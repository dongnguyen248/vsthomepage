@{ ViewBag.Title = "Index";
    Layout = "~/Views/Shared/_Layout.cshtml"; }


<div class="content-detail of-x" style="width:900px;margin-left:20%;">
    <div class="col-xs-10">
        <div style="background-image:url('/media/images/banner.jpg');width:900px;height:50px ;margin-bottom: 14px;background-repeat: no-repeat;" class="Std_Font_B">
            <div style="position:relative;">
                <p style="position:absolute;color:white;margin-left:30px;line-height: 50px; font-size:20pt"><b>  Biểu Mẫu Đăng Ký Nhà Cung Cấp</b></p>
                <p style="font-size:12px;right:16px;position:absolute;color:white;margin-right:50px;margin-bottom:30px;margin-top: 20px;">*Type Austenite</p>
            </div>
        </div>
        <div class="row">
            <button type="button" class="btn btn-primary btn-md" data-toggle="modal" data-target="#registerModal" style="float:right;">
                Đăng ký
            </button>
            <table class="table">
                <thead>
                    <tr>
                        <th>SEQ</th>
                        <th>Title</th>
                        <th>Writer</th>
                        <th>Created Date</th>
                        <th>Status</th>
                    </tr>
                </thead>
                <tbody>
                    <tr>
                        <td>1</td>
                        <td><a href="#">Cty THHH A</a></td>
                        <td>Nguyen van teo</td>
                        <td>20/10/2021</td>
                        <td>Progress</td>
                    </tr>
                    <tr>
                        <td>2</td>
                        <td><a href="#">CTy CPTMDT B</a></td>
                        <td>Nguyen Van Ty</td>
                        <td>23/05/2021</td>
                        <td>Completed</td>

                    </tr>

                </tbody>
            </table>
        </div>

    </div>
</div>

<div class="modal fade" tabindex="-1" role="dialog" aria-labelledby="gridSystemModalLabel" id="registerModal">
    <div class="modal-dialog" role="document">
        <div class="modal-content">
            <div class="modal-header">
                <button type="button" class="close" data-dismiss="modal" aria-label="Close"><span aria-hidden="true">&times;</span></button>
                <h4 class="modal-title" id="gridSystemModalLabel">Đăng Ký Nhà Cung Cấp</h4>
            </div>
            <div class="modal-body">
                <form>
                    <div class="form-group">
                        <label for="name-company" class="control-label">Tên Công Ty:</label>
                        <input type="text" class="form-control" id="name-company">
                    </div>
                    <div class="form-group">
                        <label for="date-company" class="control-label">Ngày Thành Lập:</label>
                        <input type="text" class="form-control" id="date-company">
                    </div>
                    <div class="form-group">
                        <label for="who-company" class="control-label">Người Đại Diện:</label>
                        <input type="text" class="form-control" id="who-company">
                    </div>
                    <div class="form-group">
                        <label for="product-company" class="control-label">Sản Phẩm Chính:</label>
                        <input type="text" class="form-control" id="product-company">
                    </div>
                    <div class="form-group">
                        <label for="customer-company" class="control-label">Khách Hàng Chính:</label>
                        <input type="text" class="form-control" id="customer-company">
                    </div>
                    <div class="form-group">
                        <label for="person-company" class="control-label">Người Phụ Trách:</label>
                        <input type="text" class="form-control" id="person-company" placeholder="Số điện thoại công ty, số điện thoại người phụ trách, email">
                    </div>
                    <div class="form-group">
                        <label for="message-text" class="control-label">Question:</label>
                        <textarea class="form-control" id="message-text"></textarea>
                    </div>
                    <div class="form-group" id="fileupload">
                        @*<label for="person-company" class="control-label">File Đính Kèm:(Giấy phép đầu tư, Giấy phép đăng ký kinh doanh)</label>*@
                        <label class="floatR btn btn-default">
                            <b>+</b><span> Add Contents</span>
                        </label>
                    </div>
                    <span id="inputhidden"></span>
                </form>

            </div>
            <div class="modal-footer">
                <button type="button" class="btn btn-default" data-dismiss="modal">Close</button>
                <button type="button" class="btn btn-primary">Save changes</button>
            </div>
        </div><!-- /.modal-content -->
    </div><!-- /.modal-dialog -->
</div>



