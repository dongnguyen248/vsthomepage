<!DOCTYPE html PUBLIC "-//W3C//DTD XHTML 1.0 Transitional//EN" "http://www.w3.org/TR/xhtml1/DTD/xhtml1-transitional.dtd">
<html class="no-js" lang="en">
<head>
    <meta charset="utf-8" />
    <meta http-equiv="x-ua-compatible" content="ie=edge" />
    <title>Posco VST Admin Dashboard</title>
    <link rel="icon" type="image/png" href="~/media/images/icon/P.png" />
    <meta name="viewport" content="width=device-width, initial-scale=1" />
    <link rel="shortcut icon" type="image/png" href="assets/images/icon/favicon.ico" />
    <link rel="stylesheet" href="~/Areas/Admin/assets/css/bootstrap.min.css" />
    <link rel="stylesheet" href="~/Areas/Admin/assets/css/font-awesome.min.css" />
    <link rel="stylesheet" href="~/Areas/Admin/assets/css/themify-icons.css" />
    <link rel="stylesheet" href="~/Areas/Admin/assets/css/metisMenu.css" />
    <link rel="stylesheet" href="~/Areas/Admin/assets/css/owl.carousel.min.css" />
    <link rel="stylesheet" href="~/Areas/Admin/assets/css/slicknav.min.css" />
    <link rel="stylesheet" href="~/Areas/Admin/assets/css/typography.css" />
    <link rel="stylesheet" href="~/Areas/Admin/assets/css/default-css.css" />
    <link rel="stylesheet" href="~/Areas/Admin/assets/css/styles.css" />
    <link rel="stylesheet" href="~/Areas/Admin/assets/css/responsive.css" />
    <link rel="stylesheet" href="~/Content/jquery-ui-1.12.1/jquery-ui.min.css" />
    <link rel="stylesheet" href="~/Content/DataTables/css/jquery.dataTables.min.css" />
    <script src="~/Areas/Admin/assets/js/vendor/modernizr-2.8.3.min.js"></script>
</head>

<body>
    <div class="error-area ptb--100 text-center">
        <div class="container">
            <div class="error-content">
                <p>@ViewBag.EmailResponseMsg</p>
                @Html.ActionLink("Back to Dashboard", "Index", "Customer")
                <p>This message will auto close after 10 seconds</p>
            </div>
        </div>
    </div>
    <footer>
        <div class="footer-area">
            <p>Copyright &copy; 2020 POSCO VST</p>
        </div>
    </footer>
    <script src="~/Areas/Admin/assets/js/vendor/jquery-2.2.4.min.js"></script>
    <script src="~/Areas/Admin/assets/js/popper.min.js"></script>
    <script src="~/Areas/Admin/assets/js/bootstrap.min.js"></script>
    <script src="~/Areas/Admin/assets/js/owl.carousel.min.js"></script>
    <script src="~/Areas/Admin/assets/js/metisMenu.min.js"></script>
    <script src="~/Areas/Admin/assets/js/jquery.slimscroll.min.js"></script>
    <script src="~/Areas/Admin/assets/js/jquery.slicknav.min.js"></script>
    <script src="~/Areas/Admin/assets/js/plugins.js"></script>
    <script src="~/Areas/Admin/assets/js/scripts.js"></script>
    <script src="~/Content/ckeditor/ckeditor.js"></script>
    <script src="~/Content/jquery-ui-1.12.1/jquery-ui.min.js"></script>
    <script src="~/Content/DataTables/js/jquery.dataTables.min.js"></script>
    <script type="text/javascript">
        setTimeout(function () {
            self.close();
        }, 10000);
    </script>
</body>
</html>